    <screens>
        <name>Error</name>
        <label>Error</label>
        <locationX>752</locationX>
        <locationY>352</locationY>
        <allowBack>false</allowBack>
        <allowFinish>true</allowFinish>
        <allowPause>false</allowPause>
        <fields>
            <name>ErrorDetails</name>
            <fieldText>Sorry, but an error has occurred. Support has been notified.

Details:
{!$Flow.FaultMessage}</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
    </screens>
    <screens>
        <name>scrConfirmInstall</name>
        <label>Confirm Installation Details</label>
