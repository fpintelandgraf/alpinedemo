    <externalSharingModel>Private</externalSharingModel>
    <fields>
        <fullName>Disable_time_off_requests__c</fullName>
        <defaultValue>false</defaultValue>
        <description>This checkbox is used to allow the TOM administrator to disable a user from making time-off requests.</description>
        <externalId>false</externalId>
        <label>Disable time-off requests</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>
    </fields>
    <fields>
        <fullName>Employee_Number__c</fullName>
        <caseSensitive>false</caseSensitive>
        <externalId>true</externalId>
        <label>Employee Number</label>
        <length>20</length>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <fields>
        <fullName>Note2__c</fullName>
        <externalId>false</externalId>
        <formula>&quot;Total Floating Holidays Available  =  Total Floating Holidays Accrued - Pending Floating Holiday Requests&quot; &amp; BR() &amp; BR() &amp; &quot;Please note that the Floating Holiday balances shown may not be 100% accurate.  The actual balances are stored in the payroll system.&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>&amp;nbsp;Note</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Note__c</fullName>
        <externalId>false</externalId>
        <formula>&quot;The balances shown do not include future Time-Off accruals.  The actual balances are stored in the payroll system.&quot;  &amp;  BR() &amp; BR() &amp;   &quot;PTO Available  =  PTO Accrued - PTO Pending&quot; &amp;  BR() &amp;    &quot;If you believe your PTO Accrued shown is incorrect, please file an IT Ticket to request a correction.&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Note</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PTO_Balance_Days__c</fullName>
        <externalId>false</externalId>
        <formula>ROUND( PTO_Balance_Hours__c / 8, 2)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>PTO Balance (Days)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>PTO_Balance_Hours__c</fullName>
        <externalId>false</externalId>
        <formula>Total_PTO_Hours_Accrued_2__c  -  Pending_PTO_Balance_Hours__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total PTO Available (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Payroll_System__c</fullName>
        <deleteConstraint>SetNull</deleteConstraint>
        <externalId>false</externalId>
        <label>Payroll System</label>
        <referenceTo>Payroll_System__c</referenceTo>
        <relationshipName>Time_Off_Info</relationshipName>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Lookup</type>
    </fields>
    <fields>
        <fullName>Pending_PTO_Balance_Hours__c</fullName>
        <defaultValue>0</defaultValue>
        <externalId>false</externalId>
        <label>Pending PTO Requests (Hours)</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_PTO_Accrued__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(  Total_PTO_Hours_Accrued_2__c  ) &amp; &quot; hours  (&quot;  &amp; TEXT( ROUND(Total_PTO_Hours_Accrued_2__c / 8, 2) ) &amp; &quot; days)&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>PTO Accrued</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_PTO_Available__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT(PTO_Balance_Hours__c) &amp; &quot; hours (&quot; &amp; TEXT(ROUND( PTO_Balance_Hours__c / 8, 2)) &amp; &quot; days)&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>PTO Available</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_PTO_Hours_Accrued_2__c</fullName>
        <defaultValue>0</defaultValue>
        <externalId>false</externalId>
        <label>Total PTO Hours Accrued</label>
        <precision>7</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Pending_PTO_Requests__c</fullName>
        <externalId>false</externalId>
        <formula>TEXT( Pending_PTO_Balance_Hours__c ) &amp; &quot; hours  (&quot; &amp; TEXT( ROUND(Pending_PTO_Balance_Hours__c / 8, 2)) &amp; &quot; days)&quot;</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>PTO Pending</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Total_Time_Off_Available_Hours__c</fullName>
        <externalId>false</externalId>
        <formula>PTO_Balance_Hours__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Total Time-Off Available (Hours)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Number</type>
        <unique>false</unique>
    </fields>
