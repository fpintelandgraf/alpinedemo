{
navigateToUrl(&apos;/p/process/Submit?retURL=%2F{!Time_Off_Request__c.Id}&amp;id={!Time_Off_Request__c.Id}&apos;);
} else {
window.alert(&quot;Pending, approved, processed, and canceled requests cannot be submitted for approval.&quot;);
}</url>
